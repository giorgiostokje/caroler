{
    "name": "caroler/caroler",
    "description": "An extensible Discord bot with optional Laravel integration.",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "Giorgio Stokje",
            "email": "giorgio.stokje@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "ratchet/pawl": "^0.3.4",
        "react/event-loop": "^1.1",
        "react/socket": "^1.5",
        "symfony/console": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Caroler\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caroler\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Caroler": "Caroler\\Laravel\\Facade"
            }
        }
    }
}
